{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {

    }
  },
  "permissions": ["webRequest","scripting", "storage"],
  "icons": {

  },
  "web_accessible_resources": [
    {
      "resources": ["js/*.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions" : [
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ]

}